<Sysmon schemaversion="4.60">
  <EventFiltering>
    <RuleGroup name="" groupRelation="or">
      <ProcessAccess onmatch="exclude">
        <Rule groupRelation="and">
          <TargetImage condition="contains all">C:\Users\;\AppData\Local\Programs\signal-desktop\Signal.exe</TargetImage>
          <GrantedAccess condition="contains any">0x1410;0x3000</GrantedAccess>
        </Rule>
      </ProcessAccess>
    </RuleGroup>
  </EventFiltering>
</Sysmon>
